						case 'store':
							text = inventoryDetail.store ? stores[inventoryDetail.store].name : '';
							break;
						case 'title':
							text = inventoryDetail.itemName;
							break;
						case 'sku':
							text = inventoryDetail.customSku;
							break;
						case 'barcode':
							text = inventoryDetail.itemBarcode;
							break;
						case 'cartonBarcode':
							text = inventoryDetail.cartonBarcode;
							break;
