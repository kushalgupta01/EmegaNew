function indivDecreaseValue(e) {
	let input = e.target.parentNode.querySelector('#indiQty');
	let value = parseInt(input.value, 10);
    value = isNaN(value) ? 0: value;
    value < 1 ? value = 1 : '';
    value--;

    input.value = value;
}

function indivIncreaseValue(e) {
    let input = e.target.parentNode.querySelector('#indiQty');
    let value = parseInt(input.value, 10);
    value = isNaN(value) ? 0: value;
    value++;

    input.value = value;
}

